module: "github.com/sample/repo" #the name of the git repo where your spec project resides
version: "v0.0.1"
specDir: "./specs/Types"
specDir: "./specs/Services"
specFormat: "yaml" #set to yaml or json
dependencies:
  - "github.com/theNorstroem/furoBaseTypes v1.0.0" # The importer looks for all **/*.type.spec files recursive The importer looks for all **/*.service.spec files recursive
dependenciesDir: installedSpecs
muSpec:
  types:
    - "./muspec/**types.yaml"
  services:
    - "./muspec/**services.yaml"
commands:
  taskTest: "scripts/test.sh"
  publishNpm: "scripts/test.sh"
  genTranscoder: "scripts/test.sh"
flows:
  default:
    - muSpec2Spec
    - muService2Spec
  prototyping:
    - muSpec2Spec
    - muService2Spec
    - genMessageProtos
    - genServiceProtos
    - gen_protoc_commands
    - exec_protoc_messages
    - exec_protoc_services
    - gen_transcoder
    - genEsModule
build:
  proto:
    targetDir: "./dist/protos/Messages" #Hint: add this to your proto include path
  esModule:
    targetFile: "./dist/env.js"